            </select>
        </div>

        <div>
            <span class="account_span">답변</span>
        </div>
        <div class="input_container">
            <input type="text" class="account_input" id="account_answer">
        </div>

        <a href="/account/3" class="btn btn-secondary">이전</a>
        <button id="account_4_next" class="btn btn-primary">확인</button>
